{
  "name": "blueocean-dashboard",
  "version": "0.0.1",
  "description": "Dashboard for Jenkins Blue Ocean",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jenkinsci/blueocean-plugin.git"
  },
  "scripts": {
    "start-storybook": "start-storybook --static-dir ./target,./node_modules/@jenkins-cd/design-language/dist/assets/,./node_modules/@jenkins-cd/blueocean-core-js/dist/assets/",
    "storybook": "npm run start-storybook -- -p 9001",
    "storybook:port": "npm run start-storybook -- -p ",
    "lint": "gulp lint",
    "lint:fix": "gulp lint --fixLint",
    "lint:watch": "gulp lint:watch --continueOnLint",
    "test": "gulp test",
    "test:fast": "gulp test:fast",
    "test:debug": "node --debug-brk ./node_modules/.bin/gulp test:debug",
    "test:watch": "gulp test:debug",
    "bundle": "gulp bundle",
    "bundle:watch": "gulp bundle:watch",
    "mvnbuild": "gulp lint bundle",
    "mvnbuild:fast": "gulp bundle",
    "mvntest": "gulp test"
  },
  "devDependencies": {
    "@jenkins-cd/eslint-config-jenkins": "0.0.2",
    "@jenkins-cd/js-builder": "0.0.63",
    "@jenkins-cd/js-builder-jest": "0.0.4",
    "@kadira/storybook": "2.35.3",
    "@types/bluebird": "3.5.24",
    "@types/bluebird-global": "3.5.9",
    "@types/react": "16.7.7",
    "babel": "6.23.0",
    "babel-core": "6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chai": "^4.2.0",
    "create-hmac": "1.1.7",
    "enzyme": "^3.7.0",
    "eslint": "5.9.0",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-react": "^7.11.1",
    "gulp": "3.9.1",
    "gulp-mocha": "^6.0.0",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "mocha": "5.2.0",
    "react-addons-test-utils": "15.6.2",
    "reactify": "1.1.1",
    "skin-deep": "1.2.0",
    "ts-jest": "23.10.5",
    "ts-node": "7.0.1",
    "tsify": "4.0.1",
    "typescript": "3.1.6"
  },
  "dependencies": {
    "@jenkins-cd/blueocean-core-js": "0.0.171",
    "@jenkins-cd/design-language": "0.0.666",
    "@jenkins-cd/js-modules": "0.0.10",
    "@jenkins-cd/js-extensions": "0.0.44",
    "@jenkins-cd/preferences": "0.0.5",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "bluebird": "3.5.3",
    "immutable": "3.8.2",
    "isomorphic-fetch": "2.2.1",
    "keymirror": "0.1.1",
    "lodash.debounce": "4.0.8",
    "mobx": "5.6.0",
    "mobx-react": "5.4.2",
    "mobx-utils": "5.1.0",
    "moment": "2.22.2",
    "moment-duration-format": "2.2.2",
    "react": "16.6.3",
    "react-addons-css-transition-group": "15.6.2",
    "react-addons-update": "15.6.2",
    "react-autocomplete": "1.8.1",
    "react-dom": "16.6.3",
    "react-i18next": "8.3.8",
    "react-router": "4.3.1",
    "recharts": "1.4.1"
  },
  "jenkinscd": {
    "export": [
      "reselect"
    ],
    "import": [
      "immutable@any",
      "keymirror@any",
      "moment@any",
      "moment-duration-format@any",
      "react@any",
      "react-addons-css-transition-group",
      "react-dom@any",
      "react-redux@any",
      "react-router@any",
      "redux-thunk@any"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "browserify": {
    "extensions": [
      ".js",
      ".es6",
      ".jsx",
      ".hbs",
      ".ts",
      ".tsx"
    ],
    "plugin": [
      [
        "tsify"
      ]
    ]
  }
}
