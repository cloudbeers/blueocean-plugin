{
  "name": "blueocean-web",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jenkinsci/blueocean-plugin.git"
  },
  "scripts": {
    "lint": "gulp lint",
    "lint:fix": "gulp lint --fixLint",
    "test": "gulp test",
    "test:watch": "gulp test:watch",
    "bundle": "gulp bundle",
    "bundle:watch": "gulp bundle:watch",
    "mvnbuild": "gulp lint bundle && browserify src/main/js/ie/iepolyfills.js > target/classes/org/jenkins/ui/jsmodules/blueocean-web/iepolyfills.js",
    "mvnbuild:fast": "gulp bundle",
    "mvntest": "gulp test"
  },
  "devDependencies": {
    "@jenkins-cd/js-builder": "0.0.63",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "bluebird-global": "1.0.1",
    "create-hmac": "1.1.7",
    "eslint": "5.9.0",
    "eslint-plugin-react": "^7.11.1",
    "giti": "1.1.7",
    "gulp": "3.9.1",
    "gulp-typescript": "4.0.2",
    "jest": "23.6.0",
    "jquery-detached": "2.1.4-v4",
    "ncp": "2.0.0",
    "reactify": "1.1.1",
    "ts-jest": "23.10.5",
    "tsify": "4.0.1",
    "typescript": "3.1.6",
    "zombie": "6.1.3"
  },
  "dependencies": {
    "@jenkins-cd/blueocean-core-js": "0.0.171",
    "@jenkins-cd/design-language": "0.0.666",
    "@jenkins-cd/js-modules": "0.0.10",
    "@jenkins-cd/js-extensions": "0.0.44",
    "history": "4.7.2",
    "immutable": "3.8.2",
    "keymirror": "0.1.1",
    "mobx": "5.6.0",
    "mobx-react": "5.4.2",
    "mobx-react-devtools": "6.0.3",
    "moment": "2.22.2",
    "react": "16.6.3",
    "react-addons-css-transition-group": "15.6.2",
    "react-dom": "16.6.3",
    "react-i18next": "8.3.8",
    "react-redux": "5.1.1",
    "react-router": "4.3.1",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0"
  },
  "jenkinscd": {
    "import": [
      "@jenkins-cd/blueocean-core-js@any",
      "@jenkins-cd/design-language@any",
      "@jenkins-cd/js-extensions@any",
      "moment@any",
      "react@any",
      "react-addons-css-transition-group@any",
      "react-dom@any",
      "react-router@any"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "browserify": {
    "extensions": [
      ".js",
      ".es6",
      ".jsx",
      ".hbs",
      ".ts",
      ".tsx"
    ],
    "plugin": [
      [
        "tsify"
      ]
    ]
  }
}
